<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="Entities.History" table="HISTORY">
        <!--<composite-id class="Entities.HistoryPK">-->
        <composite-id mapped="false">
            <key-property name="userid" column="USERID"/>
            <key-property name="mid" column="MID"/>
            <key-property name="viewtime" column="VIEWTIME"/>
        </composite-id>
    </class>
</hibernate-mapping>